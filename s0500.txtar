env HOME=$WORK${/}home
[windows] env USERPROFILE=$WORK\home
[windows] env LOCALAPPDATA=$WORK\appdata

[windows] exec sed -i -e "s/\r//g" expected.yaml
[darwin] exec sed -i '' 's/\r//g' expected.yaml
[linux] exec sed -i 's/\r//g' expected.yaml

exec cue import --package main input.yaml
exec cue eval --out=yaml --concrete update.cue
cmp stdout expected.yaml

-- input.yaml --
x:
 p: 3
z: 2

-- update.cue --
package main

x: p: 4
z: 2

-- expected.yaml --
x:
  p: 4
z: 2
